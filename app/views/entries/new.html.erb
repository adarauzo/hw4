<h1>New Entry</h1>

<form action="/entries" method="post" enctype="multipart/form-data"> 
  <input type="hidden" name="place_id" value="<%= params["place_id"] %>">


  <p>
    <label for="entry_title_input">Title</label>
    <input type="text" name="entry[title]" id="entry_title_input"> 
  </p>

  <p>
    <label for="entry_description_input">Description</label>
    <input type="text" name="entry[description]" id="entry_description_input">
  </p>

  <p>
    <label for="entry_occurred_on_input">Occurred On</label>
    <input type="date" name="entry[occurred_on]" id="entry_occurred_on_input">
  </p>

  <p>
    <label for="entry_image_input">Image</label>
    <input type="file" name="entry[image]" id="entry_image_input">
  </p>

  <button>Save</button>
</form>
